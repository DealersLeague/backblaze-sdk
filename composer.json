{
    "name": "wbarcelosdealersleague/backblaze-sdk",
    "description": "An SDK for working with B2 cloud storage. Fork from https://github.com/obregonco/backblaze-b2",
    "keywords": ["b2", "storage", "backblaze", "cloud", "filesystem", "backup", "backblaze-b2", "backblaze-api"],
    "homepage": "https://bitbucket.org/walterbarcelosDL/backblaze-sdk",
    "license": "LGPL-3.0-or-later",
    "type": "library",
    "authors": [
       {
            "name": "Walter Barcelos",
            "email": "walter@dealersleague.com",
            "homepage": "https://dealersleague.com"
    }
    ],
    "require": {
        "php": ">=7.0.0",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/cache": ">=5.1.0",
        "illuminate/container": ">=5.1.0",
        "illuminate/filesystem": ">=5.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {
            "dealersleague\\B2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dealersleague\\B2\\Tests\\": "tests/"
        }
    }
}
